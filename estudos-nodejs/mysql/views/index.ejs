<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Document</title>
  </head>
  <body>
    <h1>Listar usuários</h1>

    <% users.forEach(user => { %>
    <p>#<%- user.id %></p>
    <p>Nome: <%- user.nome %></p>
    <p>Idade: <%- user.idade %></p>
    <a href="/<%- user.id %> ">Descrição</a>
    <% }) %>
  </body>
</html>
